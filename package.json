{
  "name": "ReactToEveryThing",
  "version": "0.0.1",
  "description": "",
  "author": "Mike Barkmin <mbarkmin@gmail.com> (http://barkmin.eu)",
  "license": "GPL-3.0",
  "keywords": [
    "mobile",
    "native",
    "immutable",
    "javascript",
    "react-native",
    "starter-kit",
    "webpack",
    "babel",
    "eslint",
    "greenkeeper",
    "es2015"
  ],
  "scripts": {
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build": "grunt build",
    "coverage": "istanbul cover _mocha -- mocha.config.js $(find src -name '*.tests.js' ! -ipath '*node_modules*')",
    "electron": "grunt serve-electron",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "lint": "eslint src",
    "package:android": "cd android && ./gradlew assembleRelease",
    "package:desktop": "grunt build && electron-packager dist/electron ReactToEveryThing --out dist --all && grunt clean:electronDist",
    "plop": "plop",
    "react-native": "node node_modules/react-native/local-cli/cli.js start",
    "start": "npm run web",
    "test": "mocha mocha.config.js $(find src -name '*.tests.js' ! -ipath '*node_modules*')",
    "travis": "npm run lint && npm test && npm run coverage",
    "web": "grunt serve-web",
    "web:dist": "grunt serve-web:dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikebarkmin/react-to-everything"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2 <4"
  },
  "src": ".",
  "dist": "dist",
  "dependencies": {
    "i18n-js": "^1.0.0",
    "electron-prebuilt": "^1.0.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-native": "^0.41.2",
    "react-native-i18n": "0.1.1",
    "react-router": "^2.3.0"
  },
  "devDependencies": {
